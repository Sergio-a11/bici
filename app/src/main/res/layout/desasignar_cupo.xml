<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DesasignarCupo">

    <ImageButton
        android:id="@+id/btnNumSerieDAC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="145dp"
        android:src="@drawable/ic_bx_search_alt"
        app:layout_constraintStart_toEndOf="@+id/edtNumSerieDAC"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="152dp"
        android:text="Serie #"
        android:textColor="#FFFFFF"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edtNumSerieDAC"
        android:layout_width="209dp"
        android:layout_height="42dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="143dp"
        android:background="@drawable/edt_texto_campos"
        android:gravity="center"
        android:ems="10"
        android:inputType="number"
        android:maxLength="45"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="57dp"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/edtNombreDAC"
        android:layout_width="211dp"
        android:layout_height="50dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/edt_texto_campos"
        android:editable="false"
        android:ems="10"
        android:gravity="center"
        android:inputType="none"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/edtNumSerieDAC" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="52dp"
        android:text="Type"
        android:textColor="#FFFFFF"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <EditText
        android:id="@+id/edtTipoDAC"
        android:layout_width="210dp"
        android:layout_height="43dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edt_texto_campos"
        android:ems="10"
        android:gravity="center"
        android:inputType="none"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/edtNombreDAC" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:text="Color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <EditText
        android:id="@+id/edtColorDAC"
        android:layout_width="212dp"
        android:layout_height="40dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/edt_texto_campos"
        android:editable="false"
        android:ems="10"
        android:gravity="center"
        android:inputType="none"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/edtTipoDAC" />

    <Button
        android:id="@+id/btnDesasignarCupo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="117dp"
        android:layout_marginTop="45dp"
        android:text="Unasign Slot"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtColorDAC" />

    <Button
        android:id="@+id/btnVolverDAC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="28dp"
        android:text="Exit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDesasignarCupo" />

</androidx.constraintlayout.widget.ConstraintLayout>